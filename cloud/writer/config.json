{
  "database_data": [
    {
      "help": "Settings for Clickhouse Database (data)",
      "user": "default",
      "node": "local",
      "pass": "",
      "name": "qryn",
      "table_prefix": "",
      "host": "clickhouse-server",
      "_port": "9000",
      "_read_timeout": 30,
      "_write_timeout": 30,
      "strategy": "failover",
      "primary": true,
      "debug": true
    }
  ],
  "http_settings": {
    "help": "Settings for the cloki-writer Webapp Server",
    "host": "0.0.0.0",
    "port": 3101,
    "prefork": false,
    "root": "/usr/local/qryn-writer/dist",
    "gzip": true,
    "gzip_static": true,
    "api_prefix": "loki",
    "debug":true,
    "websocket": {
      "enable": false
    }
  },
  "https_settings": {
    "help": "SSL settings for cloki-writer Webapp Server",
    "enable": false,
    "host": "0.0.0.0",
    "port": 3200,
    "min-tls-version": "TLS1.2",
    "max-tls-version": "TLS1.4",
    "http_redirect": false,
    "cert": "/usr/local/qryn-writer/tls/cert.pem",
    "key": "/usr/local/qryn-writer/tls/key.pem"
  },
  "http_connect": {
    "_help": "connect timeout in seconds",
    "connect_timeout": 10
  },
  "log_settings": {
    "path": "/usr/local/qryn-writer/log",
    "level": "debug",
    "stdout": true,
    "json": true,
    "name": "qryn-writer.log",
    "syslog": false,
    "syslog_level": "LOG_INFO",
    "syslog_uri": "",
    "max_age_days": 7,
    "rotation_hours": 24
  },
  "system_settings": {
    "help": "Settings for cloki-writer logs",
    "hostname": "home",
    "url": "http://127.0.0.1",
    "hash_type": "bernstein",
    "uuid": "",
    "db_bulk": 1000,
    "db_timer": 0.2,
    "buffer_size": 80000
  },
  "api_settings": {
    "enable_token_access": true
  },
  "auth_settings": {
    "_comment": "The type param can be internal, ldap",
    "type": "internal",
    "_help:token_expire": "the settings in minutes",
    "token_expire": 1200
  }
}
